#!/bin/bash
/usr/bin/virt-install --ram {{item.mem}} --vcpus {{item.cpu}} --os-variant {{item.os.variant}} --disk path={{item.disk.path}}/{{item.name}}.qcow2,device=disk,bus=virtio,format=qcow2 --noautoconsole --vnc --network network:provisioning,mac={{item.net.prov_mac}} --network network:default --name {{item.name}} --dry-run --print-xml  > /tmp/{{item.name}}.xml; 
virsh define --file /tmp/{{item.name}}.xml; 
